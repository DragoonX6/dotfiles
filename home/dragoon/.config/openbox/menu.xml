<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/3.4/menu">
	<menu id="apps-games-menu" label="Games">
		<item label="Narcissu 1st &amp; 2nd">
			<action name="Execute">
				<execute>&quot;/home/dragoon/Desktop/Narcissu 1st &amp; 2nd.desktop&quot;</execute>
			</action>
		</item>
		<item label="Open Broadcaster Software">
			<action name="Execute">
				<execute>primusrun obs</execute>
			</action>
		</item>
		<item label="Steam">
			<action name="Execute">
				<execute>primusrun steam</execute>
			</action>
		</item>
	</menu>
	<menu id="apps-editors-menu" label="Editors">
		<item label="MEdit">
			<action name="Execute">
				<execute>medit</execute>
			</action>
		</item>
		<item label="Qt Creator">
			<action name="Execute">
				<execute>qtcreator</execute>
			</action>
		</item>
		<item label="Qt Creator (primusrun)">
			<action name="Execute">
				<execute>primusrun qtcreator</execute>
			</action>
		</item>
		<item label="Sublime Text 3">
			<action name="Execute">
				<execute>subl3</execute>
			</action>
		</item>
	</menu>
	<menu id="apps-net-menu" label="Internet">
		<item label="Deluge">
			<action name="Execute">
				<execute>deluge</execute>
			</action>
		</item>
		<item label="Mumble">
			<action name="Execute">
				<execute>mumble</execute>
			</action>
		</item>
		<item label="Shutter">
			<action name="Execute">
				<execute>shutter</execute>
			</action>
		</item>
		<item label="Skype">
			<action name="Execute">
				<execute>skype</execute>
			</action>
		</item>
		<item label="Teamspeak">
			<action name="Execute">
				<execute>/home/dragoon/Downloads/TeamSpeak3-Client-linux_amd64/ts3client_runscript.sh</execute>
			</action>
		</item>
		<item label="Teamviewer">
			<action name="Execute">
				<execute>/home/dragoon/Downloads/teamviewer/tv_bin/script/teamviewer</execute>
			</action>
		</item>
	</menu>
	<menu id="apps-multimedia-menu" label="Multimedia">
		<item label="Pulseaudio Volume Control">
			<action name="Execute">
				<execute>pavucontrol</execute>
			</action>
		</item>
	</menu>
	<menu id="apps-terminal-menu" label="Terminals">
		<item label="Xfce4 Terminal">
			<action name="Execute">
				<execute>xfce4-terminal</execute>
			</action>
		</item>
	</menu>
	<menu id="system-menu" label="System">
		<item label="Appearance">
			<action name="Execute">
				<execute>lxappearance</execute>
			</action>
		</item>
		<item label="Change Desktop Background">
			<action name="Execute">
				<execute>nitrogen --no-recurse</execute>
			</action>
		</item>
		<item label="NVidia Settings">
			<action name="Execute">
				<execute>optirun -b none nvidia-settings -c :8</execute>
			</action>
		</item>
		<item label="Openbox Menu Editor">
			<action name="Execute">
				<execute>obmenu</execute>
			</action>
		</item>
		<item label="Openbox Configuration Manager">
			<action name="Execute">
				<command>obconf</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="Tint2 configuration">
			<action name="Execute">
				<execute>tint2conf</execute>
			</action>
		</item>
		<separator/>
		<item label="Reconfigure Openbox">
			<action name="Reconfigure"/>
		</item>
	</menu>
	<menu id="root-menu" label="Openbox 3">
		<item label="Run Program">
			<action name="Execute">
				<execute>xfce4-appfinder</execute>
			</action>
		</item>
		<separator/>
		<item label="Terminal">
			<action name="Execute">
				<execute>xfce4-terminal</execute>
			</action>
		</item>
		<item label="Web Browser">
			<action name="Execute">
				<execute>firefox</execute>
			</action>
		</item>
		<item label="File Manager">
			<action name="Execute">
				<execute>thunar</execute>
			</action>
		</item>
		<item label="Text Editor">
			<action name="Execute">
				<execute>subl3</execute>
			</action>
		</item>
		<item label="Media Player">
			<action name="Execute">
				<execute>smplayer</execute>
			</action>
		</item>
		<separator/>
		<menu id="apps-games-menu"/>
		<menu id="apps-editors-menu"/>
		<menu id="apps-net-menu"/>
		<menu id="apps-multimedia-menu"/>
		<menu id="apps-terminal-menu"/>
		<separator/>
		<menu id="system-menu"/>
		<separator/>
		<item label="Exit">
			<action name="Execute">
				<execute>beelogout logout</execute>
			</action>
		</item>
		<item label="Reboot">
			<action name="Execute">
				<execute>beelogout reboot</execute>
			</action>
		</item>
		<item label="Shutdown">
			<action name="Execute">
				<execute>beelogout shutdown</execute>
			</action>
		</item>
	</menu>
</openbox_menu>
